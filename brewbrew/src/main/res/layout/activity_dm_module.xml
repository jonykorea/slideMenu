<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.brewbrew.activities.demo.DmModuleActivity"
    android:orientation="vertical">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="IntroActivity"
        android:onClick="onBtnIntroActivity"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BrewbrewMainActivity"
        android:onClick="onBtnBrewbrewMainActivity"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DmWidgetMainActivity"
        android:onClick="onBtnDmWidgetMainActivity"/>



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DaumMapMainActivity"
        android:onClick="onBtnDmDaumMapMainActivity"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GridLayoutMainActivity"
        android:onClick="onBtnDmGridLayoutMainActivity"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="KakaoLinkMainActivity"
        android:onClick="onBtnDmKakaoLinkMainActivity"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SlidingDialogMainActivity"
        android:onClick="onBtnDmSlidingDialogMainActivity"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SlidnigUpPanelActivity"
        android:onClick="onBtnDmSlidnigUpPanelActivity"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TutorialActivity"
        android:onClick="onBtnTutorialActivity"/>

</LinearLayout>
