<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2F4F5">

    <LinearLayout
        android:onClick="true"
        android:id="@+id/title_btn_menu"
        android:orientation="horizontal"
        android:background="@drawable/selector_f2f4f5_d9dbdc"
        android:gravity="center"
        android:layout_width="@dimen/size_48.5"
        android:layout_height="@dimen/size_48.5">

        <ImageButton

            android:duplicateParentState="true"
            android:layout_width="@dimen/size_20"
            android:layout_height="@dimen/size_12.5"
            android:background="@drawable/icon_widget_navy" />

    </LinearLayout>

</RelativeLayout>
